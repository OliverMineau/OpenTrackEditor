<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="CustomBottomNavigationView" parent="Widget.MaterialComponents.BottomNavigationView">
        <item name="itemPaddingTop">0dp</item>
        <item name="itemPaddingBottom">0dp</item>
        <item name="android:background">@color/background_dark</item>
    </style>

    <style name="CustomBottomNavigationView2" parent="Widget.MaterialComponents.BottomNavigationView">
        <item name="android:background">@color/background_dark</item>
    </style>

    <style name="PopupWindow_Animation_SlideUp">
        <item name="android:windowEnterAnimation">@anim/slide_in_up</item>
        <item name="android:windowExitAnimation">@anim/slide_out_down</item>
    </style>

    <style name="CustomSlider" parent="Widget.Material3.Slider">
        <item name="thumbTint">@color/black</item>
        <item name="trackColorActive">@color/black</item>
        <item name="trackColorInactive">@color/grey_white</item>
        <item name="tickColor">@color/orange</item>
    </style>


</resources>